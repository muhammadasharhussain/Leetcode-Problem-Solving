int n;
int arr[32];
int hammingWeight(int n) {

    for(int i=0;i<32;i++){
        if(n%2==0){
            arr[i]=0;
            n=n/2;
        }
        else{
            arr[i]=1;
            n=n/2;
        }
    }
    int count=0;
    for(int i=0;i<32;i++){
        if(arr[i]==1){
            count++;
        }
    }
    return count;
}
